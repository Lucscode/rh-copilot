{
  "version": 2,
  "name": "rh-copilot",
  "routes": [
    {
      "src": "/styles.css",
      "dest": "/frontend/styles.css"
    },
    {
      "src": "/app.js",
      "dest": "/frontend/app.js"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/index.html"
    }
  ],
  "functions": {
    "frontend/**": {
      "includeFiles": "**"
    }
  }
}
