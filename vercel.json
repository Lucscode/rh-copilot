{
  "version": 2,
  "name": "rh-copilot",
  "public": false,
  "buildCommand": "echo 'Build complete'",
  "outputDirectory": "frontend",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
